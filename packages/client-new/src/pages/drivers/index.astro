---
import type { DatabaseTypes } from "f1racepanel-common";
import BaseLayout from "../../layouts/BaseLayout.astro";
import { getAllDrivers } from "../../services/drivers";

const drivers = await getAllDrivers();
---

<BaseLayout title="Drivers">
  <p>All Drivers Page</p>
  {
    drivers.map((driver: DatabaseTypes.Driver) => {
      return <h2>{driver.name}</h2>;
    })
  }
</BaseLayout>
