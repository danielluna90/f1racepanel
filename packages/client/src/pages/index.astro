---
import { ResponseTypes } from "f1racepanel-server";
import BaseLayout from "../layouts/BaseLayout.astro";
import { getFromV1API } from "../services/api";
import LatestRace from "../components/home/LatestRace.astro";

const latestWeekend = await getFromV1API(
  "gpweekend/latest",
  ResponseTypes.GPWeekend
);
---

<BaseLayout title="Home">
  {latestWeekend.content && <LatestRace weekend={latestWeekend.content} />}
</BaseLayout>

<style></style>
